{
  "author": "Scott Sauyet <scott@sauyet.com> (scott.sauyet.com)",
  "contributors": [
    "Michael Hurley <mh@buzzdecafe.com> (http://buzzdecafe.com)",
    "Scott Sauyet <scott@sauyet.com> (http://fr.umio.us)",
    "David Chambers <dc@davidchambers.me> (http://davidchambers.me)",
    "Graeme Yeates <yeatesgraeme@gmail.com> (https://github.com/megawac)"
  ],
  "name": "ramda",
  "description": "A practical functional library for JavaScript programmers.",
  "version": "0.22.1",
  "homepage": "http://ramdajs.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ramda/ramda.git"
  },
  "main": "src",
  "scripts": {
    "dist": "mkdirp dist",
    "browserify": "browserify -e src/index.js -o dist/ramda.js -s R -p headerify",
    "uglify": "uglifyjs dist/ramda.js -o dist/ramda.min.js",
    "build": "npm-run-all dist browserify uglify",
    "bench": "scripts/benchRunner",
    "bookmarklet": "scripts/bookmarklet",
    "clean": "rimraf dist coverage",
    "coverage": "istanbul cover _mocha -- --reporter spec",
    "lint": "eslint scripts/bookmarklet scripts/build src/*.js src/internal/*.js test/*.js test/**/*.js lib/sauce/*.js lib/bench/*.js",
    "test": "mocha --reporter spec",
    "tdd": "npm test -- --watch",
    "browserify:tests": "browserify test/*.js -o dist/ramda.test.js",
    "test:browser": "testem ci"
  },
  "dependencies": {},
  "devDependencies": {
    "acorn": "0.9.x",
    "benchmark": "~1.0.0",
    "browserify": "10.x.x",
    "cli-table": "0.3.x",
    "commander": "2.5.x",
    "dox": "latest",
    "envvar": "1.x.x",
    "escodegen": "1.4.x",
    "eslint": "^2.11.0",
    "handlebars": "3.0.x",
    "headerify": "^1.0.1",
    "istanbul": "^0.4.x",
    "js-yaml": "^3.2.5",
    "jsverify": "^0.7.3",
    "mkdirp": "^0.5.1",
    "mocha": "2.x.x",
    "npm-run-all": "^3.1.0",
    "q": "^1.1.1",
    "ramda": "0.17.x",
    "rimraf": "~2.3.2",
    "sanctuary": "0.7.x",
    "sinon": "^1.17.4",
    "testem": "^1.12.0",
    "uglify-js": "2.4.x",
    "xyz": "1.0.x"
  }
}
